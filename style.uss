}

.engine_icon_1 {
    background-image: url('project://database/Assets/UI/RaceOrders23.png?fileID=2800000&guid=676b9eaf89951a6479f75ea923a9d4ac&type=3#RaceOrders23');
}

.engine_icon_2 {
    background-image: url('project://database/Assets/UI/RaceOrders%20(3).png?fileID=2800000&guid=7b3f746613763fa4b97cbaf0c2122339&type=3#RaceOrders (3)');
}

.engine_button_1_icon_1 {
    background-image: url('project://database/Assets/UI/iconBonuses-Tires.png?fileID=2800000&guid=e23e12d432b9f9641879da05800cdcaa&type=3#iconBonuses-Tires');
    width: 16px;
    height: 16px;
}

.engine_button_1_icon_2 {
    background-image: url('project://database/Assets/UI/iInProgress.png?fileID=2800000&guid=e59c5ad50e43eb54a9ed8f77362db8f6&type=3#iInProgress');
    width: 16px;
    height: 16px;
}

.default:hover .buttons_img {
    background-color: rgb(89, 90, 105);
    border-left-width: 0;
    border-right-width: 0;
    border-top-width: 0;
    border-bottom-width: 0;
}

.default:focus .buttons_img {
    background-color: rgb(255, 0, 2);
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    border-right-color: rgb(171, 175, 187);
    border-top-color: rgb(171, 175, 187);
    border-bottom-color: rgb(171, 175, 187);
    border-left-color: rgb(0, 0, 0);
}

.default:active .buttons_img {
    background-color: rgb(255, 0, 2);
    border-left-width: 2px;
    border-right-width: 2px;
    border-top-width: 2px;
    border-bottom-width: 2px;
    border-right-color: rgb(171, 175, 187);
    border-top-color: rgb(171, 175, 187);
    border-bottom-color: rgb(171, 175, 187);
    border-left-color: rgb(0, 0, 0);
}
